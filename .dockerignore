!ros2_ws/src/**/*.hpp
!ros2_ws/src/**/*.h
!ros2_ws/src/**/*.py

# ============================================================================
# PROJECT-SPECIFIC EXCLUSIONS
# ============================================================================
# Exclude development notebooks
ml_training/notebooks/*.ipynb

# Exclude raw datasets (use pre-processed only)
ml_training/datasets/raw/

# Exclude simulation worlds (too large)
simulation/worlds/*.world

# Exclude test media
test/test_images/
test/test_videos/
